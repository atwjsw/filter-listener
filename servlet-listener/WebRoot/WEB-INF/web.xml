<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>servlet-listener</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <listener>
  	<listener-class>com.atwjsw.listener.MyServletContextListener</listener-class>  	
  </listener>
  <listener>
  	<listener-class>com.atwjsw.listener.MyHttpSessionListener</listener-class>  	
  </listener>
  <listener>
  	<listener-class>com.atwjsw.listener.MyServletRequestListener</listener-class>  	
  </listener>
  <listener>
  	<listener-class>com.atwjsw.attr.listener.MyServletContextAttributeListener</listener-class>  	
  </listener>
  <listener>
  	<listener-class>com.atwjsw.attr.listener.MyHttpSessionAttributeListener</listener-class>  	
  </listener>
  <listener>
  	<listener-class>com.atwjsw.attr.listener.MyServletRequestAttributeListener</listener-class>  	
  </listener>
  <context-param>
  	<param-name>initParam</param-name>
  	<param-value>atwjsw</param-value>
  </context-param>
  <session-config>
  	<session-timeout>1</session-timeout>
  </session-config>
</web-app>